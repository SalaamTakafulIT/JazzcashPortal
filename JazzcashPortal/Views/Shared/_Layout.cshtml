                   document.addEventListener('DOMContentLoaded', function () {
              // Get the current path (without query params or hash)
              const path = window.location.pathname.toLowerCase();

              // Select all navigation links in the sidebar
              const navLinks = document.querySelectorAll('.navbar-vertical-content .nav-link[href]');

              // Track if we found an active link
              let activeFound = false;
              let activeParentCollapse = null;

              navLinks.forEach(function (link) {
                let linkPath = link.getAttribute('href').toLowerCase();
                if (linkPath === path) {
                  // Remove 'active' from any previously active link
                  navLinks.forEach(l => l.classList.remove('active'));
                  // Add 'active' to the matched link
                  link.classList.add('active');
                  activeFound = true;
                  // Expand parent (collapse) if any
                  activeParentCollapse = link.closest('.collapse');
                  if (activeParentCollapse) {
                    activeParentCollapse.classList.add('show');
                    // Set trigger aria-expanded=true
                    let trigger = activeParentCollapse.previousElementSibling;
                    if (trigger && trigger.classList.contains('nav-link')) {
                      trigger.setAttribute('aria-expanded', 'true');
                    }
                  }
                }
              });

              // Collapse all other .collapse elements except the active one
              document.querySelectorAll('.navbar-vertical-content .collapse').forEach(function (collapse) {
                if (collapse !== activeParentCollapse) {
                  collapse.classList.remove('show');
                  // Set trigger aria-expanded=false
                  let trigger = collapse.previousElementSibling;
                  if (trigger && trigger.classList.contains('nav-link')) {
                    trigger.setAttribute('aria-expanded', 'false');
                  }
                }
              });
            });
        </script>
