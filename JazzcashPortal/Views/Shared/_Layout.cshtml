    <script>

        $(function () {
            var $searchInput = $('.navbar-top-search-box .search-input');
            var $staticSection = $('#static-search-sections');
            var $dynamicSection = $('#dynamic-search-results');
            var $dropdownMenu = $searchInput.closest('.navbar-top-search-box').find('.dropdown-menu');
            var $fallback = $dropdownMenu.find('.fallback');

            function showStatic() {
                $staticSection.removeClass('d-none');
                $dynamicSection.addClass('d-none').empty();
                $fallback.addClass('d-none');
            }

            function showDynamic(filtered) {
                $staticSection.addClass('d-none');
                $dynamicSection.removeClass('d-none').empty();
                if(filtered.length === 0) {
                    $fallback.removeClass('d-none');
                    return;
                }
                $fallback.addClass('d-none');
                filtered.forEach(function (item) {
                    $dynamicSection.append(
                        `<a class="dropdown-item py-2 d-flex align-items-center" href="${item.url}">
                            <span class="me-2 ${item.icon}"></span>
                            <div class="flex-1">
                                <h6 class="mb-0 text-body-highlight title">${item.title}</h6>
                            </div>
                        </a>`
                    );
                });
            }

            $searchInput.on('focus', function () {
                if (!$searchInput.val()) {
                    showStatic();
                }
                $dropdownMenu.addClass('show');
            });

            $searchInput.on('input', function () {
                var val = $(this).val().trim().toLowerCase();
                if (val.length === 0) {
                    showStatic();
                    return;
                }
                var filtered = LIS_DYNAMIC_MENU.filter(function (item) {
                    return item.title.toLowerCase().includes(val);
                });
                showDynamic(filtered);
            });

            $searchInput.on('blur', function () {
                setTimeout(function () {
                    $dropdownMenu.removeClass('show');
                }, 200);
            });
        });
